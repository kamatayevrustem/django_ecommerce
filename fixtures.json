  }
},
{
  "model": "articles.article",
  "pk": 3,
  "fields": {
    "name": "\u041a\u0430\u043a\u043e\u0439 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d \u0434\u043e 15 000 \u0440\u0443\u0431\u043b\u0435\u0439 \u0432\u044b\u0431\u0440\u0430\u0442\u044c \u0432 2019 \u0433\u043e\u0434\u0443?",
    "text": "\u042f \u0441\u0447\u0438\u0442\u0430\u044e, \u0447\u0442\u043e \u043d\u0430 \u0440\u044b\u043d\u043a\u0435 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u043e\u0432 \u0435\u0441\u0442\u044c \u0434\u0432\u0435 \u0441\u0430\u043c\u044b\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0435 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432. \u041f\u0435\u0440\u0432\u0430\u044f - \u044d\u0442\u043e \u0444\u043b\u0430\u0433\u043c\u0430\u043d\u0441\u043a\u0438\u0435 \u043c\u043e\u0434\u0435\u043b\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043d\u0435\u0441\u0443\u0442 \u0432 \u0441\u0435\u0431\u0435 \u0432\u0441\u0435 \u0441\u0430\u043c\u044b\u0435 \u043d\u043e\u0432\u044b\u0435 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0438, \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0435 \u043a\u0430\u043c\u0435\u0440\u044b, \u0432\u044b\u0441\u043e\u0447\u0430\u0439\u0448\u0443\u044e \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0438 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u0443\u044e \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0432 \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u0435, \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f\u0445 \u0438 \u0438\u0433\u0440\u0430\u0445. \u0414\u0440\u0443\u0433\u0430\u044f \u0436\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0430\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f \u2014 \u044d\u0442\u043e \u0442\u0435 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0438\u043c\u0435\u044e\u0442 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b\u0439 \u0446\u0435\u043d\u043d\u0438\u043a, \u043d\u043e \u0438\u043c\u0438 \u0432\u043f\u043e\u043b\u043d\u0435 \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u043a\u0430\u0436\u0434\u044b\u0439 \u0434\u0435\u043d\u044c, \u0442\u043e \u0435\u0441\u0442\u044c \u0443 \u043d\u0438\u0445 \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0434\u043b\u044f \u043b\u044e\u0431\u044b\u0445 \u043f\u043e\u0432\u0441\u0435\u0434\u043d\u0435\u0432\u043d\u044b\u0445 \u0437\u0430\u0434\u0430\u0447, \u043d\u0435\u043f\u043b\u043e\u0445\u0430\u044f \u043a\u0430\u043c\u0435\u0440\u0430, \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0434\u0438\u0441\u043f\u043b\u0435\u0439 \u0438 \u043f\u043e\u043b\u043d\u044b\u0439 \u043d\u0430\u0431\u043e\u0440 \u0441\u0435\u0442\u0435\u0432\u044b\u0445 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0435\u0439.\r\n\r\n\u041d\u0435\u0434\u0430\u0432\u043d\u043e \u043f\u0435\u0440\u0435\u0434\u043e \u043c\u043d\u043e\u0439 \u0432\u0441\u0442\u0430\u043b\u0430 \u0437\u0430\u0434\u0430\u0447\u0430 \u0432\u044b\u0431\u0440\u0430\u0442\u044c \u043d\u043e\u0432\u044b\u0439 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d \u0434\u043b\u044f \u043b\u044e\u0431\u0438\u043c\u043e\u0439 \u0442\u0435\u0449\u0438. \u0411\u044e\u0434\u0436\u0435\u0442 \u043e\u0431\u043e\u0437\u043d\u0430\u0447\u0430\u043b\u0441\u044f \u0442\u0430\u043a: \u043d\u0443\u0436\u043d\u043e \u043a\u0443\u043f\u0438\u0442\u044c \u0430\u043f\u043f\u0430\u0440\u0430\u0442, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0431\u044b\u043b \u0431\u044b \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0431\u044e\u0434\u0436\u0435\u0442\u043d\u044b\u043c \u043f\u043e \u0446\u0435\u043d\u0435, \u043f\u043e \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u044b\u043c \u0432 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0438. \u0411\u0435\u0437 \u0434\u043e\u043b\u0436\u043d\u043e\u0433\u043e \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u0432\u043e\u043f\u0440\u043e\u0441\u0430 \u044f \u0441\u043d\u0430\u0447\u0430\u043b\u0430 \u0440\u0435\u0448\u0438\u043b \u0443\u043b\u043e\u0436\u0438\u0442\u044c\u0441\u044f \u0432 10 000 \u0440\u0443\u0431\u043b\u0435\u0439, \u043d\u043e \u043f\u043e\u0442\u043e\u043c \u043f\u043e\u043d\u044f\u043b, \u0447\u0442\u043e \u0432 \u044d\u0442\u043e\u0439 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u043c\u044b \u0441\u0442\u043e\u043b\u043a\u043d\u0435\u043c\u0441\u044f \u0441 \u043a\u043e\u043c\u043f\u0440\u043e\u043c\u0438\u0441\u0441\u0430\u043c\u0438, \u0432 \u0447\u0438\u0441\u043b\u0435 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 HD-\u0434\u0438\u0441\u043f\u043b\u0435\u0439, \u043c\u0430\u043b\u044b\u0439 \u043e\u0431\u044a\u0435\u043c \u043f\u0430\u043c\u044f\u0442\u0438 (\u043a\u0430\u043a \u043e\u043f\u0435\u0440\u0430\u0442\u0438\u0432\u043d\u043e\u0439, \u0442\u0430\u043a \u0438 \u0432\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u043e\u0439), \u0430 \u0442\u0430\u043a\u0436\u0435 \u0441\u043b\u0430\u0431\u0435\u043d\u044c\u043a\u0438\u0439 \u0431\u044e\u0434\u0436\u0435\u0442\u043d\u044b\u0439 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u043e\u0440. \u0417\u0430\u043a\u0430\u0437\u044b\u0432\u0430\u0442\u044c \u043a\u043e\u043c\u0443-\u0442\u043e \u0447\u0442\u043e-\u0442\u043e \u0438\u0437 \u041a\u0438\u0442\u0430\u044f \u044f \u0442\u043e\u0436\u0435 \u043d\u0435 \u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0438\u043a, \u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c\u0441\u044f \u0441\u0435\u0431\u0435 \u0434\u043e\u0440\u043e\u0436\u0435, \u0435\u0441\u043b\u0438 \u0441 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u043e\u043c \u0447\u0442\u043e-\u0442\u043e \u0431\u0443\u0434\u0435\u0442 \u043d\u0435 \u0442\u0430\u043a. \u0421\u0435\u0431\u0435 \u2014 \u043f\u043e\u0436\u0430\u043b\u0443\u0439\u0441\u0442\u0430, \u0430 \u043a\u043e\u043c\u0443-\u0442\u043e \u2014 \u0442\u043e\u0447\u043d\u043e \u043d\u0435\u0442. \u041e\u0434\u043d\u0430\u043a\u043e \u0432 \u0441\u0435\u0433\u043e\u0434\u043d\u044f\u0448\u043d\u0435\u0439 \u043f\u043e\u0434\u0431\u043e\u0440\u043a\u0435 \u0431\u0443\u0434\u0435\u0442 \u043f\u0430\u0440\u0430 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0445 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u043e\u0432 \u0438\u0437 \u041f\u043e\u0434\u043d\u0435\u0431\u0435\u0441\u043d\u043e\u0439.\r\n\r\n\u0412 \u0438\u0442\u043e\u0433\u0435 \u044f \u043f\u0440\u0438\u043d\u044f\u043b \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u0431\u044e\u0434\u0436\u0435\u0442 \u0434\u043e 15 000 \u0440\u0443\u0431\u043b\u0435\u0439, \u0447\u0435\u043c \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0440\u0430\u0441\u0448\u0438\u0440\u0438\u043b \u0432\u044b\u0431\u043e\u0440, \u0430 \u0433\u043b\u0430\u0432\u043d\u043e\u0435, \u043e\u0442\u043e\u0448\u0435\u043b \u043e\u0442 \u0432\u044b\u0448\u0435\u043d\u0430\u0437\u0432\u0430\u043d\u043d\u044b\u0445 \u043a\u043e\u043c\u043f\u0440\u043e\u043c\u0438\u0441\u0441\u043e\u0432. \u0422\u0443\u0442 \u0443\u0436\u0435 \u0438 \u0434\u0438\u0441\u043f\u043b\u0435\u0438 \u0441 \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u0435\u043c FullHD+, \u0438 \u043f\u0430\u043c\u044f\u0442\u044c \u043e\u0442 3 \u0413\u0411 \u043e\u043f\u0435\u0440\u0430\u0442\u0438\u0432\u043d\u0430\u044f \u0438 \u043e\u0442 32 \u0413\u0411 \u0432\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u0430\u044f, \u0438 \u0447\u0438\u043f\u0441\u0435\u0442\u044b \u0441\u0440\u0435\u0434\u043d\u0435\u0433\u043e \u0438 \u0441\u0440\u0435\u0434\u043d\u0435-\u0432\u044b\u0441\u043e\u043a\u043e\u0433\u043e \u043a\u043b\u0430\u0441\u0441\u0430. \u0412 \u043e\u0431\u0449\u0435\u043c, \u0432\u0441\u0435 \u0447\u0442\u043e \u043d\u0443\u0436\u043d\u043e \u0438 \u0437\u0430 \u043e\u043f\u0442\u0438\u043c\u0430\u043b\u044c\u043d\u044b\u0435, \u043a\u0430\u043a \u044f \u0441\u0447\u0438\u0442\u0430\u044e, \u0434\u0435\u043d\u044c\u0433\u0438. \u041b\u0443\u0447\u0448\u0438\u0435 \u0438\u0437 \u044d\u0442\u0438\u0445 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432 \u044f \u0432\u043a\u043b\u044e\u0447\u0438\u043b \u0432 \u0441\u0435\u0433\u043e\u0434\u043d\u044f\u0448\u043d\u044e\u044e \u043f\u043e\u0434\u0431\u043e\u0440\u043a\u0443.",
